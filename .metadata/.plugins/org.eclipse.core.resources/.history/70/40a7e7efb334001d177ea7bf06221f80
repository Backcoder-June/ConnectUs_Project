<mapper namespace="reservation.ReservationDAO">


	<!-- 전체게시물 조회 (페이징) -->
	<insert id="insertReservation" >
		insert into reservation (buyerid, sellerid, startRental, endRental, boardid )
		values (#{buyerid}, #{sellerid}, #{startRental}, #{endRental}, #{boardid})
	</insert>

	
</mapper>